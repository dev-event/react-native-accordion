{
  "name": "@dev-event/react-native-accordion",
  "version": "2.0.5",
  "description": "A performant interactive collapsed view with fully configurable options",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-accordion.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "release": "release-it",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "clean": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install && yarn start --reset-cache"
  },
  "keywords": [
    "react-native",
    "expo",
    "ios",
    "android",
    "collapsed",
    "accordion",
    "expanded",
    "animated view"
  ],
  "repository": "https://github.com/dev-event/react-native-accordion",
  "author": "dev-event <effectwaater@gmail.com> (https://github.com/dev-event)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dev-event/react-native-accordion/issues"
  },
  "homepage": "https://github.com/dev-event/react-native-accordion#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@release-it/conventional-changelog": "^2.0.1",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^26.0.0",
    "@types/invariant": "^2.2.34",
    "@types/react": "^17.0.5",
    "@types/react-native": "^0.64.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.10.1",
    "typescript": "^4.2.4",
    "react-native-reanimated": "2.4.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": ">=2.1.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "react-native-redash": "16.0.11",
    "react-native-svg": "12.3.0"
  }
}
